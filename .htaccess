<files .htaccess="">
order allow,deny
deny from all
</files>
RewriteEngine On

# Auth routes
RewriteRule ^AuthLogin$ backend/login.php [L]
RewriteRule ^Register$ backend/register.php [L]
RewriteRule ^ForgotPassword$ backend/forgot-pwd.php [L]
RewriteRule ^Auth$ backend/auth.php [L]
RewriteRule ^Logout$ backend/logout.php [L]


# UI
RewriteRule ^Home$ index.php [L]
RewriteRule ^Main$ frontend/main.php [L]
RewriteRule ^Ebooks$ frontend/ebooks.php [L]
RewriteRule ^Ebook$ frontend/ebook.php [L]
RewriteRule ^Video$ frontend/video.php [L]
RewriteRule ^TapChi$ frontend/tapchi.php [L]
RewriteRule ^EbookDetail$ frontend/ebook_detail.php [L]


#admin
RewriteRule ^Upload$ upload.php [L]
RewriteRule ^Search$ search.php [L]
RewriteRule ^Manage/Ebooks$ admin/manage_ebooks.php [L]